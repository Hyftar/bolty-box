<!DOCTYPE html>
<html>
  <head>
    <title>
      <%= full_title(yield(:title)) %>
    </title>
    <%= csrf_meta_tags %>
    <link href="https://fonts.googleapis.com/css?family=Lobster+Two:700i|Roboto+Condensed:300,300i,400,400i,700,700i" rel="stylesheet">
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
      <%= render 'layouts/header' %>
      <div class="center">
        <%= render partial: 'layouts/notice', locals: {notice: notice} %>
        <%= yield %>
      </div>
      <%= render 'layouts/footer' %>
  </body>
</html>
